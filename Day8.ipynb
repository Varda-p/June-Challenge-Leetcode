    "class Solution(object):\n",
    "    def isPowerOfTwo(self, n):\n",
    "        \"\"\"\n",
    "        :type n: int\n",
    "        :rtype: bool\n",
    "        \"\"\"\n",
    "        if n == 1:\n",
    "            return True\n",
    "        elif n%2 == 0:\n",
    "            n = n //2\n",
    "            return True\n",
    "        else:\n",
    "            return False"
